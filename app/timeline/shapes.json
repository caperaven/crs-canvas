{
  "shapes": [
    {
      "shapeType": "rect",
      "fromField": "workStartedOn",
      "toField": "completedOn",
      "condition": "completedOn > requiredBy ? 'row_range4' : 'row_range2'"
    },
    {
      "shapeType": "range_indicator",
      "fromField": "startOn",
      "toField": "completeBy"
    },
    {
      "shapeType": "pillar",
      "fromField": "receivedOn",
      "toField": "requiredBy"
    }
  ],
  "theme": {
    "clearColor": "#ffffff",
    "header_bg": "#FFFFFF",
    "secondary_header_bg": "#F4F4F4",
    "header_offset_bg": "#F4F4F4",
    "header_text": "#424242",
    "header_border": "#DDDDDD",
    "offset_row_bg": "#F9F9F9",
    "row_range1": "#C8E5E1",
    "row_range2": "#16A085",
    "row_range3": "#757575",
    "row_range4": "#FF0000",
    "row_selection": "#B9D8EC",
    "row_selection_border": "#0276C2"
  },
  "records": [
    "<regular style=\"color: #424242\">Code: ${code}</regular>",
    "<icon style=\"color: #424242\">98</icon> <bold style=\"color: #424242\">[${assetCode}]</bold> <regular style=\"color: #424242\">${assetDescription}</regular>",
    "<icon style=\"color: #424242\">97</icon> <bold style=\"color: #424242\">[${siteCode}]</bold> <regular style=\"color: #424242\">${siteDescription}</regular>"
  ]
}
